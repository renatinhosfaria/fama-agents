{
  "name": "fama-agents",
  "version": "0.1.0",
  "description": "Sistema completo de agentes de codificação e skills com Claude Agent SDK",
  "type": "module",
  "bin": {
    "fama": "dist/bin/fama.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx bin/fama.ts",
    "start": "node dist/bin/fama.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "lint": "eslint src/",
    "ci": "pnpm run lint && pnpm run typecheck && pnpm run build && pnpm run test"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "claude",
    "agent",
    "sdk",
    "coding",
    "ai"
  ],
  "files": [
    "dist",
    "agents",
    "skills",
    ".claude-plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.29",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3",
    "ora": "^9.1.0",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
}
